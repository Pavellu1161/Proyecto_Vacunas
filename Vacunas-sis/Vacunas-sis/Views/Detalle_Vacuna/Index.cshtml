@model IEnumerable<Vacunas_sis.Models.Detalle_Vacuna>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_aplicacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Numero_dosis_aplicada)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Numero_dosis_restantes)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_proxima_dosis)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vacunas)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_aplicacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numero_dosis_aplicada)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Numero_dosis_restantes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_proxima_dosis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Vacunas.Edad_aplicable)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_detalle_vacuna">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_detalle_vacuna">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_detalle_vacuna">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
