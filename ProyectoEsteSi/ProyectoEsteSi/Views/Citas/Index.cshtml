@model IEnumerable<ProyectoEsteSi.Models.Cita>


@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Dosis.Id_nino)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_proxima)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dosis)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Dosis.Pacientes.Numero_identidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Fecha_proxima)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dosis.IdDosis)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdCita">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdCita">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdCita">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
